                        a.btn.btn-dark.btn-xl.ml-3.mr-3(href="/deeplearning") Test Engine
                    .col-sm
                        h4 English News Sentiment
                        a.btn.btn-dark.btn-xl.ml-3.mr-3(href="/englishSentiment") Test Engine
                .row.mt-5
                    .col-sm
                        h4 API document
                        a.btn.btn-dark.btn-xl.ml-3.mr-3(href="http://api.sgcslab.com") API document

        div.content-section.bg-primary.text-white.text-center
            div.container
                .content-section-heading
                    h2.mb-5 Contact
            div.row
                div.col-lg-3.col-md-6.mb-5.mb-lg-0
                    span.service-icon.rounded-circle.mx-auto.mb-3
                        i.icon-call-end 
                    p 02-6338-1880
                div.col-lg-3.col-md-6.mb-5.mb-lg-0
                    span.service-icon.rounded-circle.mx-auto.mb-3
